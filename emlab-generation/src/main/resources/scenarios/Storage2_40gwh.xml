<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- Bean post processor to persist created beans -->
	<bean class="agentspring.graphdb.PersistingBeanPostProcessor" />


	<!-- Decarbonization Model -->
	<bean id="decarbonizationModel" class="emlab.gen.domain.agent.DecarbonizationModel"
		p:name="Decarbonization Model" p:simulationLength="${simulationLength}"
		p:co2TradingImplemented="false" p:realRenewableDataImplemented="true"
		p:co2BankingIsImplemented="false"
		p:deletionOldPPDPBidsAndCashFlowsEnabled="true" p:deletionAge="4"
		p:centralForecastBacklookingYears="5"
		p:centralForecastingYear="3" p:centralCO2TargetReversionSpeedFactor="3"
		p:centralPrivateDiscountingRate="0.05" p:centralCO2BackSmoothingFactor="0"
		p:stabilityReserveIsActive="false"
		p:stabilityReserveFirstYearOfOperation="100"
		p:stabilityReserveBankingFirstYear="0.8"
		p:stabilityReserveBankingSecondYear="0.5"
		p:stabilityReserveBankingThirdYear="0.2"
		p:feedInPremiumImplemented="true"
		p:renewableTenderSchemeImplemented="false">
	</bean>

	<!-- Interconnector -->
	<!-- https://www.rvo.nl/sites/default/files/2015/05/IP01_Inpassingsplan%20DW380%20Toelichting%20en%20regels%2020150415%20-%20EXCL%20BIJLAGEN_vKT.pdf
	says 2450 MW in (2014) with an expected increase to 3950 MW in 2017-->
	<bean id="interconnectorNetherlandsGermany" class="emlab.gen.domain.technology.Interconnector"
		p:capacity="0">
		<property name="connections">
			<set>
				<ref bean="nlNode1" />
				<ref bean="deNode1" />
			</set>
		</property>
	</bean>

	<!-- Regulator -->
<bean id="regulatorNL" class="emlab.gen.domain.agent.Regulator" p:name="Regulator NL"
p:numberOfYearsLookingBackToForecastDemand="5"
p:equityInterestRate="0.0797" p:loanInterestRate="0.0428"
		p:equityRatePriceRiskComponent="0.00"
		p:debtRatioOfInvestments="0.7">
<property name="zone" ref="nl"/>
</bean>

<!-- Feed in Premium -->


<bean id="renewableSupportScheme" class="emlab.gen.domain.policy.renewablesupport.RenewableSupportFipScheme" p:name="FeedInPremium"
p:supportSchemeDuration="20"
p:emRevenuePaidExpost="true"
p:futureSchemeStartTime="2"
p:technologySpecificityEnabled="false"
p:avgElectricityPriceBasedPremiumEnabled="false"
p:costContainmentMechanismEnabled="false">
<property name="regulator" ref="regulatorNL"/>
<property name="zone" ref="nl"/>
<property name="powerGeneratingTechnologiesEligible">
			<set>
				<ref bean="photovoltaicPGT" />
				<ref bean="windOffshorePGT" />
				<ref bean="windPGT" />
			</set>
</property>
</bean>

<bean id="biasFactorNL1PV" class="emlab.gen.domain.policy.renewablesupport.BiasFactor"
p:feedInPremiumBiasFactor="1"
p:degressionFactor="0.06">
<property name="node" ref="nlNode1"> </property>
<property name="scheme" ref="renewableSupportScheme"></property>
<property name="technology" ref = "photovoltaicPGT" ></property>
</bean>

<bean id="biasFactorNL1Wind" class="emlab.gen.domain.policy.renewablesupport.BiasFactor"
p:feedInPremiumBiasFactor="1"
p:degressionFactor="0.06">
<property name="node" ref="nlNode1"> </property>
<property name="scheme" ref="renewableSupportScheme"></property>
<property name="technology" ref = "windPGT" ></property>
</bean>

<bean id="biasFactorNL1Windoffshore" class="emlab.gen.domain.policy.renewablesupport.BiasFactor"
p:feedInPremiumBiasFactor="1"
p:degressionFactor="0.06">
<property name="node" ref="nlNode1"> </property>
<property name="scheme" ref="renewableSupportScheme"></property>
<property name="technology" ref = "windOffshorePGT" ></property>
</bean>




